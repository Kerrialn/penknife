{
  "name": "kerrialn/penknife",
  "require": {
    "php": "^8.2",
    "symfony/console": "^7.1",
    "symfony/process": "^7.1",
    "phpstan/phpstan": "^1.12"
  },
  "require-dev": {
    "symplify/easy-coding-standard": "^12.3",
    "rector/rector": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "KerrialNewham\\Penknife\\": "src"
    }
  },
  "scripts": {
    "ps": "vendor/bin/phpstan",
    "rector": "vendor/bin/rector",
    "check-cs": "vendor/bin/ecs",
    "fix-cs": "vendor/bin/ecs --fix"
  }
}
